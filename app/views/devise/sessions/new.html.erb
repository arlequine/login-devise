  <div class="col-md-12">
    <div class="nb">
      <div class="form-group">
      <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
      <%= devise_error_messages! %>
        <h2 class="center">Log in</h2>
        <div class="form-group">
          <%= f.label :email%><br />
          <%= f.email_field :email, autofocus: true, class: "form-control", placeholder: "Enter mail" %>
        </div>

        <div class="form-group">
          <%= f.label :password %><br />
          <%= f.password_field :password, autocomplete: "off", class: "form-control", placeholder: "Enter password"  %>
        </div>

        <% if devise_mapping.rememberable? -%>
          <div class="field">
            <%= f.check_box :remember_me, :class => "checkbox" %>
            <%= f.label :remember_me %>
          </div>
        <% end -%>

        <div class="actions">
          <%= f.submit "Log in", :class => "btn btn-block" %>
        </div>
      <% end %>
      </div>
      <div class="center">
        <%= render "devise/shared/links" %>
        <div class="center or">OR</div>
        <h3 class="center">Sign In Using</h3>
      </div>
        <div class="social">
          <a href="#" class="google"><i class="fa fa-google"></i> &nbsp; Login with Google</a>
        </div>
    </div>
  </div>
